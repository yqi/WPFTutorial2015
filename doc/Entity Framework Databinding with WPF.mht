=0A=
        private void Window_Loaded(object sender, RoutedEventArgs e)=0A=
        {=0A=
            System.Windows.Data.CollectionViewSource categoryViewSource =3D=0A=
                =
((System.Windows.Data.CollectionViewSource)(this.FindResource("categoryVi=
ewSource")));=0A=
